<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pc.ChineseChow">

    <uses-sdk
        android:maxSdkVersion="26"
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".HomeChefParty"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <uses-permission android:name="android.permission.INTERNET" />

        <activity
            android:name=".RecipeFeed"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Search"
            android:label="ChineseChow" />
        <activity
            android:name=".Upload"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".EmailSignUpActivity" />
        <activity android:name=".GoogleSignIn" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ViewRecipeInSearch" />
        <activity android:name=".NameUpload" />
        <activity android:name=".CookPrepUpload" />
        <activity android:name=".ImgUpload" />
        <activity android:name=".IngredientsUpload" />
        <activity android:name=".ViewRecipeInFeed"></activity>
    </application>

</manifest>